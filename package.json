{
  "name": "packing-list-mobile-app",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "find-unused": "knip",
    "prebuild": "npm run typecheck && npm run lint && knip",
    "ios:release": "./scripts/ios-release.sh",
    "ios:release:clean": "npx expo prebuild --clean && ./scripts/ios-release.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "expo": "~54.0.32",
    "expo-checkbox": "~5.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "firebase": "^11.0.2",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-navigation": "^8.7.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@biomejs/biome": "^2.3.13",
    "@types/hoist-non-react-statics": "^3.3.7",
    "@types/lodash": "^4.17.23",
    "@types/prop-types": "^15.7.15",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.0",
    "knip": "^5.82.1",
    "typescript": "~5.9.2"
  }
}
